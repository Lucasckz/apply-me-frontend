    {
      "env": {
        "browser": true,
        "es2021": true,
        "jest": true
      },
      "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "plugin:prettier/recommended"
      ],
      "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "plugins": [
        "react",
        "react-hooks",
        "prettier"
      ],
      "rules": {
        "react/react-in-jsx-scope": "off"
      }
    }